<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<BorderPane fx:id="pane" prefHeight="650.0" prefWidth="950.0"
  xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="org.example.onesteponestamp.javafx.AdminLogInController"
  stylesheets="@/org/example/onesteponestamp/css/adminLogin.css">
  <center>
    <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
      <children>
        <VBox prefHeight="200.0" prefWidth="100.0">
          <children>
            <TextField fx:id="idField" promptText="아이디"/>
            <PasswordField fx:id="passwordField" promptText="비밀번호"/>
            <Button fx:id="loginButton" text="Log In"/>
          </children>
        </VBox>
      </children>
    </HBox>
  </center>
  <left>
    <Button fx:id="homeButton">
      <graphic>
        <ImageView fx:id="homeIcon" image="@/org/example/onesteponestamp/images/home.png"/>
      </graphic>
    </Button>
  </left>
</BorderPane>
