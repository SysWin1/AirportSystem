<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="mainLayOut" prefHeight="650.0" prefWidth="950.0"
  stylesheets="@/org/example/onesteponestamp/css/adminMain.css" xmlns="http://javafx.com/javafx/22"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="org.example.onesteponestamp.javafx.AdminMainController">
  <top>
    <HBox fx:id="menu" alignment="CENTER_LEFT" prefHeight="39.0">
      <children>
        <Button fx:id="autoApplyButton" minHeight="-Infinity" minWidth="-Infinity" prefHeight="38.0"
          prefWidth="237.5" text="자동 입출국 심사 목록 조회"/>
        <Button fx:id="immigrationButton" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="38.0" prefWidth="237.5" text="입출국 목록 조회"/>
        <Button fx:id="foreignerButton" minHeight="-Infinity" minWidth="-Infinity" prefHeight="38.0"
          prefWidth="237.5" text="외국인 목록 조회"/>
        <Button fx:id="homeButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="38.0" prefWidth="237.5" text="홈으로">
          <graphic>
            <ImageView fx:id="homeIcon" fitHeight="30" fitWidth="30.0" pickOnBounds="true"
              preserveRatio="true"/>
          </graphic>
        </Button>
      </children>
    </HBox>
  </top>
</BorderPane>
